{
  "name": "adel/hackparser",
  "description": "HackLang parser written in PHP",
  "type": "library",
  "keywords": [
    "hacklang",
    "parser"
  ],
  "homepage": "<git_repository_url>",
  "authors": [
    {
      "name": "Adel Maratova",
      "email": "adel.maratova@gmail.com"
    }
  ],
  "require": {
    "nikic/PHP-Parser": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "PhpLang\\Phack\\": "lib/"
    },
    "files": [
      "lib/autoload.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "PhpLang\\Phack\\Test\\": "tests/"
    }
  },
  "bin": [
    "hackParser"
  ]
}
